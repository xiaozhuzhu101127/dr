<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="hugeBtn">
        <item name="android:textColor">@color/color_while</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">@drawable/btn_bg_huge</item>
    </style>

    <style name="iSpinner">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textScaleX">0.85</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="iWeatherLabel">
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="iWeatherDetail">
        <item name="android:lineSpacingExtra">2dp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#666666</item>
        <item name="android:layout_marginBottom">5dp</item>
    </style>

    <style name="Theme.TransparentNoTitleBar" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="Theme.NoTitleBar" parent="@android:style/Theme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="dialogWindowAnim" mce_bogus="1" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/dialog_enter_anim</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit_anim</item>
    </style>

    <style name="icompleteLabel">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:textScaleX">0.85</item>
        <item name="android:textSize">19sp</item>
        <item name="android:textColor">#333333</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <!-- 边框 -->
        <item name="android:windowIsFloating">true</item>
        <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item>
        <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 无标题 -->
        <item name="android:windowBackground">@color/transparent</item>
        <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">false</item>
        <!-- 模糊 -->
    </style>

    <style name="myRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_conf_file</item>
        <item name="android:numStars">5</item>
        <item name="android:minHeight">21dp</item>
        <item name="android:maxHeight">25dp</item>
    </style>

    <style name="myBigRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/star_conf_file_big</item>
        <item name="android:numStars">5</item>
        <item name="android:minHeight">30dp</item>
        <item name="android:maxHeight">35dp</item>
    </style>

